{
  "name": "abelapko/functional-interpreter-php",
  "description": "ANTLR-based src/parser for functional language on PHP",
  "type": "project",
  "license": "MIT",
  "keywords": [
    "php",
    "InterpreterAst"
  ],
  "require": {
    "php": "^8.0",
    "antlr/antlr4-php-runtime": "^0.9.1",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Abelapko\\Interpreter\\": "src/",
      "Abelapko\\Interpreter\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.68",
    "phpunit/phpunit": "^11.5",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "lint": "vendor/bin/phpstan analyse src --level=8",
    "cs-fix": "vendor/bin/php-cs-fixer fix",
    "test": "vendor/bin/phpunit tests"
  }
}
